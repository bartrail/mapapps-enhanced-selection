{
  "dataform-version": "1.0.0",
  "size": {
    "h": 300,
    "w": 400
  },
  "type": "gridpanel",
  "showLabels": true,
  "cols": 1,
  "children": [
    {
      "type": "checkbox",
      "field": "widgetEnabled",
      "title": "${ui.enable}",
      "tooltip": "${ui.enableTooltip}",
      "required": true
    },
    {
      "type": "numbertextbox",
      "field": "driveTime.minimum",
      "title": "${ui.driveTime.minimum.title}",
      "tooltip": "${ui.driveTime.minimum.tooltip}",
      "required": true,
      "min": 0,
      "size": {
        "l": 5,
        "t": 5,
        "w": 100
      }
    },
    {
      "type": "numbertextbox",
      "field": "driveTime.maximum",
      "title": "${ui.driveTime.maximum.title}",
      "tooltip": "${ui.driveTime.maximum.tooltip}",
      "required": true,
      "min": 0,
      "size": {
        "l": 5,
        "t": 5,
        "w": 100
      }
    },
    {
      "type": "numbertextbox",
      "field": "driveTime.interval",
      "title": "${ui.driveTime.interval.title}",
      "tooltip": "${ui.driveTime.interval.tooltip}",
      "required": true,
      "min": 0,
      "size": {
        "l": 5,
        "t": 5,
        "w": 100
      }
    },
    {
      "type": "textbox",
      "field": "geoprocessorUrl",
      "title": "Geoprocessor URL:",
      "tooltip": "URL of Geoprocessor for computing drivetime-polygons",
      "required": true,
      "size": {
        "l": 5,
        "t": 5,
        "w": 500
      }
    }
  ]
}